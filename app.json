{
	"expo": {
		"name": "ArtisanTradeNetwork",
		"slug": "ArtisanTradeNetwork",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/images/logo-4x.png",
			"resizeMode": "native",
			"backgroundColor": "#ffffff"
		},
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			}
		},
		"web": {
			"favicon": "./assets/images/favicon.png",
			"bundler": "metro"
		},
		"scheme": "your-app-scheme",
		"plugins": [
			"expo-router",
			["expo-document-picker", { "icloudContainerEnvironment": "Production" }],
			[
				"expo-image-picker",
				{
					"photosPermission": "The app access your photos to let you share them with your friends"
				}
			]
		]
	}
}
